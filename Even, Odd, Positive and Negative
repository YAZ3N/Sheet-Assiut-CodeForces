#include <iostream>
using namespace std;
int main() {
 
	int n;
	cin >> n;
	int counteven = 0, countodd = 0, countpos = 0, countneg = 0;
	for (int i = 0; i < n; i++) {
		int x;
		cin >> x;
 
		if (x % 2 == 0) {
			counteven++;
		}
		else {
			countodd++;
		}
		if (x > 0) {
			countpos++;
		}
		if (x < 0) {
			countneg++;
		}
	}
	cout << "Even: " << counteven << endl;
	cout << "Odd: " << countodd << endl;
	cout << "Positive: " << countpos << endl;
	cout << "Negative: " << countneg;
}
