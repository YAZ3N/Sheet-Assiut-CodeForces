#include <iostream>
using namespace std;
int main() {
	int n, a, b;
	cin >> n >> a >> b;
	int sum = 0;
 
	for (int i = 1; i <= n; i++) {
		int Sumdigit = 0;
		int temp = i;
		while (temp > 0) {
			Sumdigit += temp % 10;
			temp /= 10;
		}
		if (Sumdigit >= a && Sumdigit <= b) {
			sum += i;
		}
	}
	cout << sum;
}
