#include <iostream>
using namespace std;
int main() {
 
	long long n;
	cin >> n;
	long long reverse = 0;
	long ongorginal = n;
	while (n > 0) {
		reverse = reverse * 10 + n % 10;
		n = n / 10;
	}
	cout << reverse << endl;
 
	if (ongorginal == reverse) {
		cout << "YES";
   }
	else {
		cout << "NO";
	}
 
}
