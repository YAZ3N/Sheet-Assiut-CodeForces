#include <iostream>
using namespace std;
int main() {
	while (true) {
		int n, m;
		cin >> n >> m;
		if (m <= 0 || n <= 0) {
			break;
		}
		if (n > m) {
			int temp = n;
			n = m;
			m = temp;
		}
 
		int sum = 0;
		for (int i = n; i <= m; i++) {
			cout << i << " ";
			sum += i;
		}
		cout << "sum =" << sum << endl;
	}
 
}
