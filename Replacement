#include <iostream>
using namespace std;
int main() {
	int n; cin >> n;
	int* array = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> array[i];
		if (array[i] < 0) {
			array[i] = 2;
		}
		else if (array[i] > 0) {
			array[i] = 1;
		}
		else {
			array[i] = array[i];
		}
	}
	for (int i = 0; i < n; i++) {
		cout << array[i] << " ";
	}
}
