#include <iostream>
using namespace std;
int main() {
	long a, b,c;
	cin >> a >> b>>c;
	long min=0, max = 0;
	if (a >= b && a >= c) {
		max = a;
	}
	else if (b >= a && b >= c) {
		max = b;
	}
	else {
		max = c;
	}
	if (a <= b && a <= c) {
		min = a;
	}
	else if (b <= a && b <= c) {
		min = b;
	}
	else {
		min = c;
	}
	cout << min << " " << max;
}
