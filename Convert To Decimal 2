#include <iostream>
using namespace std;
int main() {
	int n; cin >> n;
	while (n--) {
		long long x;
		cin >> x;
		int counter = 0;
		while (x > 0) {
			if (x % 2 == 1)
				counter++;
				x = x / 2;
			
		}
		long long resalt = 0;
		for (int i = 0; i < counter; i++) {
			resalt = resalt * 2 + 1;
		}
		cout << resalt<<endl;
	}
}
