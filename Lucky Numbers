#include <iostream>
using namespace std;
int main() {
	int a, b;
	cin >> a >> b;
	bool found = false;
	for (int i = a; i <= b; i++) {
		bool flag = true;
		int x = i;
		while (x > 0) {
			int digit = x % 10;
			if (digit != 4 && digit != 7) {
				flag = false;
				break;
			}
			x = x / 10;
		}
		if (flag) {
			cout << i << " ";
			found = true;
		}
	}
	if (!found) {
		cout << -1;
	}
}
